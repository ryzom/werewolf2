<script src="script.as" name="leaf_vertex_program">
	<function name="getShader" return="string">
		<argument id="0" type="int" name="numLights">
			<binding map="ENGINE" property="ENGINE.NumberOfLights"/>
		</argument>
		<argument id="1" type="int" name="numShadowCasters">
			<binding map="LOCAL" property="LOCAL.NumberOfShadowCasters"/>
		</argument>
		<argument id="2" type="int" name="numIntersectors">
			<binding map="LOCAL" property="LOCAL.NumberOfIntersectors"/>
		</argument>
	</function>
</script>

<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="shared.Keyword" table="keywords">
		<id name="id" column="kwID" type="long">
			<generator class="identity"/>
		</id>
		<property name="keyword" column="keyword" unique="true"/>
    </class>

</hibernate-mapping>